{
  "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
  "version": "2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "CopyTree",
          "version": "0.0.0",
          "informationUri": "https://copytree.dev",
          "rules": [
            {
              "id": "file-discovered",
              "name": "FileDiscovered",
              "shortDescription": {
                "text": "A file was discovered by CopyTree."
              },
              "fullDescription": {
                "text": "CopyTree enumerated this file in the selected scope based on the configured profile and filters."
              },
              "helpUri": "https://copytree.dev",
              "defaultConfiguration": {
                "level": "note"
              },
              "properties": {
                "category": "file-discovery",
                "tags": [
                  "discovery",
                  "enumeration"
                ]
              }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "file-discovered",
          "level": "note",
          "message": {
            "text": "File discovered: index.js"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "index.js",
                  "uriBaseId": "%SRCROOT%"
                },
                "region": {
                  "startLine": 1,
                  "endLine": 2
                }
              }
            }
          ],
          "properties": {
            "size": <SIZE>,
            "modified": "<TIMESTAMP>",
            "isBinary": false,
            "encoding": "utf8"
          }
        },
        {
          "ruleId": "file-discovered",
          "level": "note",
          "message": {
            "text": "File discovered: README.md"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "README.md",
                  "uriBaseId": "%SRCROOT%"
                },
                "region": {
                  "startLine": 1,
                  "endLine": 2
                }
              }
            }
          ],
          "properties": {
            "size": <SIZE>,
            "modified": "<TIMESTAMP>",
            "isBinary": false,
            "encoding": "utf8"
          }
        },
        {
          "ruleId": "file-discovered",
          "level": "note",
          "message": {
            "text": "File discovered: src/test.js"
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/test.js",
                  "uriBaseId": "%SRCROOT%"
                },
                "region": {
                  "startLine": 1,
                  "endLine": 4
                }
              }
            }
          ],
          "properties": {
            "size": <SIZE>,
            "modified": "<TIMESTAMP>",
            "isBinary": false,
            "encoding": "utf8"
          }
        }
      ],
      "invocations": [
        {
          "executionSuccessful": true,
          "endTimeUtc": "<TIMESTAMP>",
          "workingDirectory": {
            "uri": "file://<PROJECT_ROOT>"
          }
        }
      ],
      "properties": {
        "profile": "default",
        "fileCount": 3,
        "totalSize": 77,
        "git": null
      }
    }
  ]
